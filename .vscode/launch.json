    "version": "0.2.0",
    "configurations": [
        {
            "name": "go_firebase macOS",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "args": [
                "--dart-define-from-file=firebase_config/macOS.json"
            ]
        },
        {
            "name": "go_firebase iOS",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "args": [
                "--dart-define-from-file=firebase_config/iOS.json"
            ]
        },
        {
            "name": "go_firebase (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "go_firebase (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        }
    ]
}